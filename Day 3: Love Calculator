# Day 3: Love Calculator 
Print("The Love Calculator is calculating your score...") 
name1 = input() # your name
name2 = input() # their name 
combined_names = name1+ name2 
lower_names = combined_names.lower()
t = lower_names.count("t") 
r = lower_names.count("r")
u = lower_names.count("u") 
e = lower_names.count("e") 
first_digits = t + r + u + e 
l = lower_names.count("l") 
o = lower_names.count("o")
v = lower_names.count("v") 
e = lower_names.count("e") 
second_digits = l + o + v + e
score=int(str(first_digits)+str(second_digits))) 

if (score<10) or (score>90):
  print (f"Your score is {}, you go together like coke and mentos.")
elif (score>=40) or (score<=50): 
  print(f" Your score is {}, you go alright together.") 
else: 
  print(f" Your score is {})
